{
  "name": "Vocareum Anti-Anti-Paste",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Allows pasting in the Vocarum online IDE",
  "homepage_url": "",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "page action demo",
    "default_popup": "src/page_action/page_action.html"
  },
  "permissions": [
    "https://*.vocareum.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.vocareum.com/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}
